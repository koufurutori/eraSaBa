@Valentine_History(ARG)
IF CFLAG:ARG:발렌타인
	PRINTFORML 발렌타인에 %조사처리(Valentine_Chocolate(ARG),"를")% 주었다 \@ CFLAG:ARG:발렌타인 > 100?  (답례 완료)# \@
ENDIF

@Valentine_Chocolate(ARG)
#FUNCTIONS
SELECTCASE CFLAG:ARG:발렌타인 % 100
	CASE 1
		LOCALS = 블랙썬더를
	CASE 2
		LOCALS = 평범한 초콜릿을
	CASE 3
		LOCALS = 고급 초콜릿을
	CASE 4
		LOCALS = 수제 초콜릿을
ENDSELECT
RETURNF LOCALS

@PERSONALITY_TYPE(ARG)
#FUNCTIONS
SELECTCASE CFLAG:ARG:성격경향
	CASE 1
		RETURNF "심약"
	CASE 2
		RETURNF "강경"
	CASE 3
		RETURNF "솔직"
	CASE 4
		RETURNF "성실"
	CASE 5
		RETURNF "그외"
ENDSELECT
