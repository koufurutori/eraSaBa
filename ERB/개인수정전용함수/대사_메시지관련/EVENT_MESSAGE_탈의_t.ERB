
@MESSAGE_탈의윗도리
IF SHOW_TOP(TARGET) != ""
	IF TEQUIP:3 == 0; && EQUIP:TARGET:상반신속옷1
		PRINTFORM %CALLNAME:TARGET%의 
		IF EQUIP:TARGET:상반신윗도리1
			IF EQUIP:TARGET:원피스
				PRINTFORM %조사처리(CLOTHNAME(17, EQUIP:TARGET:상반신윗도리1),"와")%
				SIF EQUIP:TARGET:상반신윗도리2
					PRINTFORM %조사처리(CLOTHNAME(16, EQUIP:TARGET:상반신윗도리2),"와")%
				SIF EQUIP:TARGET:기모노
					PRINTFORM %조사처리(CLOTHNAME(14, EQUIP:TARGET:기모노),"와")%
				PRINTFORM %조사처리(CLOTHNAME(13, EQUIP:TARGET:원피스),"를")% 벗기면, 
			ELSE
				PRINTFORM %조사처리(CLOTHNAME(17, EQUIP:TARGET:상반신윗도리1),"를")%
				IF EQUIP:TARGET:상반신윗도리2
					PRINTFORM 벗기고 %CLOTHNAME(16, EQUIP:TARGET:상반신윗도리2)%
					SIF EQUIP:TARGET:기모노
						PRINTFORM %조사만처리(CLOTHNAME(16, EQUIP:TARGET:상반신윗도리2),"와")% %조사처리(CLOTHNAME(14, EQUIP:TARGET:기모노),"를")%
