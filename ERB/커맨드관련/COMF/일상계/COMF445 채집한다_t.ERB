;-----------------------------------------------------------
;현재 위치로부터 채집 스팟을 산출하는 함수
;ARG		현재 위치
;ARG:1		@ShowGatheringSpots부터의 참조용
;RETURN		0 해당 없음
;COMMENT	장소를 추가했을 경우, CASE를 변경의 일
;-----------------------------------------------------------
@GATHERING_SPOT(ARG, ARG:1)
#FUNCTION

;メインマップでの判定
SELECTCASE ARG

	CASE P22창고가는길
		RETURNF 1
	CASE P23벽가는길
		RETURNF 2
	CASE P24호수가는길
		RETURNF 3
	CASE P25꽃밭가는길
		RETURNF 4
	CASE P26감옥저택앞
		RETURNF 5
	CASE P36안뜰
		RETURNF 6
	CASE P53호수앞
		RETURNF 7
	CASE P54호숫가
		RETURNF 8
	CASE P56벽앞
		RETURNF 9
	CASE P57꽃밭앞
		RETURNF 10
	CASE P58꽃밭
		RETURNF 11
	CASE P68게스트하우스앞
		RETURNF 12

ENDSELECT

RETURNF 0

;-------------------------------------------------
;그 채집 스팟에서의 채집물의 출현 판정을 하는 함수
; 아이템 번호 ARG:0
;GATHERING_SPOT(ARG)의 돌아가 값ARG:1（채집 스팟）  의
;출현 가부를 숫자로 돌려주는 출현==1비출현==0
; 아이템을 추가했을 경우는, GROUPMATCH내에 각각 덧붙여 씀
;-------------------------------------------------
@GATHERING_PRODUCT(ARG:0, ARG:1)
#FUNCTION

;(酒造パッチ)머루665・곰팡이핀포도666を追加
SELECTCASE ARG:1
	;P22창고가는길
	CASE 1
		SIF GROUPMATCH(ARG:0, 626, 627, 628, 629, 630)
			RETURNF 1
	;P23벽가는길
	CASE 2
		SIF GROUPMATCH(ARG:0, 646, 647, 648)
			RETURNF 1
	;P24호수가는길
	CASE 3
		SIF GROUPMATCH(ARG:0, 636, 610, 611, 636, 637)
			RETURNF 1
	;P25꽃밭가는길
	CASE 4
		SIF GROUPMATCH(ARG:0, 626, 627, 628, 629, 630)
			RETURNF 1
	;P26감옥저택앞
	CASE 5
		SIF GROUPMATCH(ARG:0, 621, 622, 623, 624, 625, 631, 632, 633, 634, 635, 638)
			RETURNF 1
	;P36안뜰
	CASE 6
		SIF GROUPMATCH(ARG:0, 607, 608, 611, 616, 617, 618, 619,)
			RETURNF 1
	;P53호수앞
	CASE 7
		SIF GROUPMATCH(ARG:0, 610, 611, 636, 637, 638, 641, 642)
			RETURNF 1
	;P54호숫가
	CASE 8
		SIF GROUPMATCH(ARG:0, 606, 609, 639, 640, 641, 642, 643, 644, 645) 
			RETURNF 1
	;P56벽앞 - 버섯류
	CASE 9
		SIF GROUPMATCH(ARG:0, 600, 601, 602, 603, 604, 605, )
			RETURNF 1
	;P57꽃밭앞
	CASE 10
		SIF GROUPMATCH(ARG:0, 607, 608, 612, 613)
			RETURNF 1
	;P58꽃밭
	CASE 11
		SIF GROUPMATCH(ARG:0, 611, 612, 613, 614, 615, 616, 617, 618, 619, 620)
			RETURNF 1
	;P68게스트하우스앞
	CASE 12
		SIF GROUPMATCH(ARG:0, 611, 612, 649, 616, 617, 618, 619)
			RETURNF 1
	
ENDSELECT
RETURNF 0

;-------------------------------------------------
;채집물의 계절마다의 출현 판정
; 아이템 번호 ARG:0의 계절마다의
;출현 가부를 숫자로 돌려주는 함수 출현==1비출현==0
; 아이템을 추가했을 경우는, CASE에 덧붙여 씀
;ARG:1 に채집지점을 넣으면 아래 목록용 식으로 변환 생략가능
;-------------------------------------------------
@GATHERING_SEASON(ARG:0, ARG:1 = 0)
#FUNCTION

LOCAL = DAY:2
SIF GatheringSeason
	LOCAL = GatheringSeason

;(酒造パッチ)머루665・곰팡이핀포도666を追加
SELECTCASE LOCAL
	;봄:
	CASE 1
		SELECTCASE ARG:0
			CASE 600 TO 602, 604 TO 608, 610 TO 613, 616, 618 TO 627, 629, 631 TO 640, 647, 706
				RETURNF 1
		ENDSELECT
	;여름
	CASE 2
		SELECTCASE ARG:0
			CASE 600 TO 601, 603, 606 TO 608, 612 TO 613, 618 TO 627, 630 TO 640, 647, 665, 706
				RETURNF 1
		ENDSELECT
	;가을
	CASE 3
		SELECTCASE ARG:0
			CASE 600 TO 608, 611 TO 614, 616 TO 623, 625 TO 628, 631 TO 640, 647, 665, 706
				RETURNF 1
		ENDSELECT
		;(酒造パッチ)(天候パッチ)かびたブドウ666は寒気の強さが5の時のみ
		SIF ARG:0 == 666 && POWER_COLD_AIR == 5
			RETURNF 1
	;겨울
	CASE 4
		;天界、地底、月とキノコ群生地、虹龍洞は夏とみなして採集可能
		IF ARG:1
			LOCAL:1 = ARG:1
		ELSE
			LOCAL:1 = GATHERING_SPOT(CFLAG:MASTER:현재위치)
		ENDIF

		IF GROUPMATCH(LOCAL:1, 13, 14, 37, 63, 64, 65, 66, 67, 68)
			SELECTCASE ARG:0
				CASE 600 TO 601, 603, 606 TO 609, 612 TO 613, 618 TO 627, 630 TO 640, 647, 706
					RETURNF 1
			ENDSELECT
		ELSE
			SELECTCASE ARG:0
				CASE 606, 609, 615, 620, 631 TO 635, 637 TO 640
					RETURNF 1
			ENDSELECT
		ENDIF
ENDSELECT
RETURNF 0

