@SYSTEM_TITLE
LOADGLOBAL
RESULT = GLOBAL
SAVEGLOBAL
LOADGLOBAL

RESETBGCOLOR
IF GAMEBASE_VERSION == 0
	WINDOW_TITLE = %GAMEBASE_TITLE% %eraTW_Version%
ELSEIF GAMEBASE_VERSION < 0
	WINDOW_TITLE = %GAMEBASE_TITLE% 0.00.3-BETA
ENDIF
CLEARLINE 1
RESULT  = 1
LOCAL:2 = LINECOUNT

CALL COMPOSE_SPRITE

$TITLE_SELECT
DO
	REDRAW 0
	CLEARLINE LINECOUNT - LOCAL:2
;일단 지금 이미지 없으니까 막아둠
;	FOR LOCAL:0, 0, 35
;		HTML_PRINT "<p align='center'><img src='TW_title" + TOSTR(LOCAL:0, "000") + "'></p>"
;	NEXT
	ALIGNMENT CENTER
	CUSTOMDRAWLINE ━
	PRINTFORML %GAMEBASE_TITLE%
	PRINTFORML \@ GAMEBASE_VERSION <= 0 ? %eraTW_Version% # %TOSTR(GAMEBASE_VERSION,"0,.###")% \@
	PRINTFORML %GAMEBASE_YEAR%
;		CALL HTMLPRINTL(@"%HTMLFONT(@"<b>%HTMLSTYLE(@"%GAMEBASE_TITLE%", 2)%</b>", "Courier New", )%")
;		CALL HTMLPRINTL(@"%HTMLFONT(@"<b>%eraTW_Version%</b>", "Courier New")%")
;		CALL HTMLPRINTL(@"%HTMLFONT(@"<b>(%GAMEBASE_YEAR%)</b>", "Courier New")%")
	PRINTL
	PRINTFORML %GAMEBASE_INFO%
	PRINTFORML Updated by 경추
	CUSTOMDRAWLINE ━
	PRINTFORML [0] 시작하기
	PRINTFORML [1] 불러오기
	ALIGNMENT LEFT
	REDRAW 1
	INPUT
	CUSTOMDRAWLINE ━
	SELECTCASE RESULT
		CASE 0
			RESETDATA
			ADDCHARA 0
			CALL IS_UEMUERA()
			BEGIN FIRST
		CASE 1
			;LOADGAME
			;KR판 추가수정, 로드용 커스텀 함수
			CALL LOADGAME_KR
			GOTO TITLE_SELECT
	ENDSELECT
LOOP RESULT
