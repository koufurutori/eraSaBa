@CHARAMOVE_DATA_7(ARG, ARG:2)
LOCAL = 7
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		;나노카는 개인주의야
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;집에 안 가! 집에 안 가! 집에 안 가!

			;방에 제발 그만 처박혀!!!
			CASE P1메루루방, P2코코・한나방, P3레이아・미리아방, P4노아・안안방, P5에마・히로방, P6마고・나노카방, P7셰리・아리사방, P79대마녀방
				RETURN -10

			;저수조는 좀 덜 갈지도.
			CASE P37저수조
				RETURN -5

			;기본적으로는 밖을 돌아다니는 걸 좀 더 선호해
			CASE P22창고가는길, P23벽가는길, P24호수가는길, P25꽃밭가는길, P26감옥저택앞, P27게스트하우스가는길, P49조작실, P50환풍기통로, P51냉동고, P53호수앞, P54호숫가, P55호수, P57꽃밭앞, P58꽃밭, P64창고앞, P65샐러맨더의방, P66운디네의방, P67노움의방, P68게스트하우스앞
				RETURN 5

			;창고는 좋은 은신처지
			CASE P59창고방1, P60창고방2, P61창고방3, P62창고방4, P63창고
				RETURN 10

			;자꾸 지하 1층에 애들 처박히는 거 막기 위해서 지하 1층의 모든 구역은 좀 덜 가게 설정하겠다
			CASE P8감옥복도, P9지하1층계단, P10소각로통로, P11소각로, P12징벌방통로, P13징벌방복도, P14징벌방1, P15징벌방2, P16징벌방3, P17징벌방4, P18징벌방5, P19징벌방6, P20징벌방7, P21징벌방8
				RETURN -5

		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
		;	CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루
		;		RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
		;감옥 가 있어야 하는 시간에는 감옥에 있음. 단, 나노카는 1일차에만 이걸 따르고 그 다음부터는 지멋대로 행동.
		;본래 가야하는 시간의 약 20분 전부터 움직이게 해야 그나마 들어가는 듯...
		;감옥 시간 : 0시 ~ 6시 / 10시 ~ 12시 / 15시 ~ 17시 / 22시 ~ 24시
		IF DAY == 1 && (INRANGE(TIME, 0, 360) || INRANGE(TIME, 580, 720) || INRANGE(TIME, 880, 1020) || INRANGE(TIME, 1300, 1440))
			TCVAR:LOCAL:인력점 = P6마고・나노카방
			;근데 못 들어갔을 때를 대비해야 하니까
			;못 들어가면 인력점을 리셋하고 배회하도록... 안 그러면 무한루프 빠지네.
			;SIF DAY == 1 && (INRANGE(TIME, 0, 360) || INRANGE(TIME, 600, 720) || INRANGE(TIME, 900, 1020) || INRANGE(TIME, 1320, 1440)) && (CFLAG:7:현재위치 != P6마고・나노카방)
			;	TCVAR:LOCAL:인력점 = 0
		ENDIF

		;06:00~09:00 , 12:00~14:00, 17:00~21:00 에만 "맛없는식사" 제공
		;나노카(30분 -> 레이아의 시간과 겹침), 식사에 튀어나올 확률 20%
		;8.30 - 9 / 12 - 12.30 / 19 - 19.30
		IF (CFLAG:LOCAL:오늘의운세 % 5 == 1) && (INRANGE(TIME, 510, 540) || INRANGE(TIME, 721, 750) || INRANGE(TIME, 1140, 1170))
			TCVAR:LOCAL:인력점 = P45식당
		ENDIF

		;화요일 창고에서 언니 회상 중ㅠㅠㅠㅠㅠㅠㅠㅠㅠ 21 ~ 21.5
		IF YOUBI_MATCH(DAY, "화") && INRANGE(TIME, 1260, 1290)
			TCVAR:LOCAL:인력점 = P63창고
		ELSE
			;나노카는 뭐 알아서 샤워실 가겠지....
		ENDIF
ENDSELECT

