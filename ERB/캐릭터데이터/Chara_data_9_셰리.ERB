;IMAGE_I9_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;   RESULTSに特殊服装を格納すること
;   書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I9_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
    ;差し替え画像
    IF CHECK_NUDE(CHARA)
        ;裸
        IF FLAG:스탠딩종류
            ;顔絵
        ELSE
            ;立ち絵
        ENDIF
    ELSE
        ;服
        IF FLAG:스탠딩종류
            ;顔絵
        ELSE
            ;立ち絵
        ENDIF
    ENDIF
ELSE
    ;通常画像
    IF CHECK_NUDE(CHARA)
        ;裸
        IF FLAG:스탠딩종류
            ;顔絵
        ELSE
            ;立ち絵
        ENDIF
    ELSE
        ;服
        IF FLAG:스탠딩종류
            ;顔絵
            ;下着のみ(TEQUIP:1 = [n])
            ;SIF TEQUIP:TARGET:상반신착의상황 == 2
            SIF TEQUIP:CHARA:5
                RESULTS = _下着のみ
            ;半脱ぎ着衣(上半身はだけ:TEQUIP:3)
            SIF TEQUIP:CHARA:상반신탈의상태 == 1
                RESULTS = _半脱ぎ
            ;バニースーツ
            SIF CFLAG:CHARA:의상일시변경 == 55 || (CFLAG:CHARA:기본복장세트 == 55 && CFLAG:CHARA:의상일시변경==0)
                RESULTS = _バニー
            ;変T
            SIF (CFLAG:CHARA:의상일시변경 == 62 || (CFLAG:CHARA:기본복장세트 == 62 && CFLAG:CHARA:의상일시변경==0) ) && TEQUIP:CHARA:상반신탈의상태 == 1
                RESULTS = _地獄Ｔシャツ_半脱ぎ
            SIF CFLAG:CHARA:의상일시변경 == 62 || (CFLAG:CHARA:기본복장세트 == 62 && CFLAG:CHARA:의상일시변경==0)
                RESULTS = _地獄Ｔシャツ
            ;セーラー服
            SIF CFLAG:CHARA:의상일시변경 == 74 || (CFLAG:CHARA:기본복장세트 == 74 && CFLAG:CHARA:의상일시변경==0)
                RESULTS = _セーラー
        ELSE
            ;立ち絵
            ;下着のみ(TEQUIP:1 = [n])
            IF TEQUIP:CHARA:5
                IF CFLAG:CHARA:291
                    RESULTS = _ブラのみ
                ELSEIF TEQUIP:CHARA:하반신착의상황 <= 3
                    RESULTS = _下着のみ
                ENDIF
            ENDIF
            ;半脱ぎ着衣(上半身はだけ:TEQUIP:3)
            IF TEQUIP:CHARA:상반신탈의상태 == 1
                IF CFLAG:CHARA:291
                    RESULTS = _半脱ぎノーパン
                ELSE
                    RESULTS = _半脱ぎ
                ENDIF
            ENDIF
            
            ;バニースーツ
            SIF CFLAG:CHARA:의상일시변경 == 55 || (CFLAG:CHARA:기본복장세트 == 55 && CFLAG:CHARA:의상일시변경==0)
                RESULTS = _バニー
            
            ;変T
            IF CFLAG:CHARA:의상일시변경 == 62 || (CFLAG:CHARA:기본복장세트 == 62 && CFLAG:CHARA:의상일시변경==0)
                IF TEQUIP:CHARA:상반신탈의상태 == 1 && CFLAG:CHARA:291
                    RESULTS = _地獄Ｔシャツ_半脱ぎノーパン
                ELSEIF TEQUIP:CHARA:상반신탈의상태 == 1
                    RESULTS = _地獄Ｔシャツ_半脱ぎ
                ELSE
                    RESULTS = _地獄Ｔシャツ
                ENDIF
            ENDIF
            ;세라복
            SIF CFLAG:CHARA:의상일시변경 == 74 || (CFLAG:CHARA:기본복장세트 == 74 && CFLAG:CHARA:의상일시변경==0)
                RESULTS = _セーラー
        ENDIF
    ENDIF
ENDIF

