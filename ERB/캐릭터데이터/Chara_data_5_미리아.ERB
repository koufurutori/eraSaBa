@CHARAMOVE_DATA_5(ARG, ARG:2)
LOCAL = 5
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		;어쩔까... 노아랑 안안이랑 같이 다니게 해?
		RESULT:1 = [[노아]]
		RESULT:2 = [[안안]]
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;오지상은 애들 돌봐야하는 슬픈 인생이 있어...

			;방에 제발 그만 처박혀!!!
			CASE P1메루루방, P2코코・한나방, P3레이아・미리아방, P4노아・안안방, P5에마・히로방, P6마고・나노카방, P7셰리・아리사방, P79대마녀방
				RETURN -10

			;저수조는 좀 덜 갈지도.
			CASE P37저수조
				RETURN -10

			;오락실이랑 응접실 좋아한다.
			CASE P71오락실, P38응접실
				RETURN 5

			;자꾸 지하 1층에 애들 처박히는 거 막기 위해서 지하 1층의 모든 구역은 좀 덜 가게 설정하겠다
			CASE P8감옥복도, P9지하1층계단, P10소각로통로, P11소각로, P12징벌방통로, P13징벌방복도, P14징벌방1, P15징벌방2, P16징벌방3, P17징벌방4, P18징벌방5, P19징벌방6, P20징벌방7, P21징벌방8
				RETURN -5

		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
		;	CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루
		;		RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
		;감옥 가 있어야 하는 시간에는 감옥에 있음
		;본래 가야하는 시간의 약 20분 전부터 움직이게 해야 그나마 들어가는 듯...
		;감옥 시간 : 0시 ~ 6시 / 10시 ~ 12시 / 15시 ~ 17시 / 22시 ~ 24시
		IF INRANGE(TIME, 0, 360) || INRANGE(TIME, 580, 720) || INRANGE(TIME, 880, 1020) || INRANGE(TIME, 1300, 1440)
			TCVAR:LOCAL:인력점 = P3레이아・미리아방
			;근데 못 들어갔을 때를 대비해야 하니까
			;못 들어가면 인력점을 리셋하고 배회하도록... 안 그러면 무한루프 빠지네.
			;SIF (INRANGE(TIME, 0, 360) || INRANGE(TIME, 600, 720) || INRANGE(TIME, 900, 1020) || INRANGE(TIME, 1320, 1440)) && (CFLAG:5:현재위치 != P3레이아・미리아방)
			;	TCVAR:LOCAL:인력점 = 0
		ENDIF

		;06:00~09:00 , 12:00~14:00, 17:00~21:00 에만 "맛없는식사" 제공
		;안안 미리아 코코는 7시 30분 - 8시 30분 / 13시 - 14시 / 20시 - 21시
		;노아도 얘들이랑 같이 밥을 먹는데 33% 확률로만 그날 나옴 CFLAG:LOCAL:오늘의운세 % 3 == 1
		IF INRANGE(TIME, 450, 510) || INRANGE(TIME, 780, 840) || INRANGE(TIME, 1200, 1260)
			TCVAR:LOCAL:인력점 = P45식당
		ENDIF

		;화수목요일 18시 ~ 19시 30분까지는 안안과 오락실에서 영화 감상
		IF YOUBI_MATCH(DAY, "화수목") && INRANGE(TIME, 1080, 1170)
			TCVAR:LOCAL:인력점 = P71오락실
		;일요일 21시 ~ 21시 30분까지는 복도에서 별구경
		ELSEIF YOUBI_MATCH(DAY, "일") && INRANGE(TIME, 1080, 1170)
			TCVAR:LOCAL:인력점 = P69복도1
		ELSE
			;17:00~21:00에만 샤워실 사용 가능
			;샤워실은 랜덤 함수 돌리게 할까... 기본적으로 본인의 저녁시간은 피하도록.
			SELECTCASE CFLAG:LOCAL:오늘의운세 % 6
				CASE 0
					;17 - 17.5
					SIF INRANGE(TIME, 1021, 1050)
						TCVAR:LOCAL:인력점 = P35샤워실
				CASE 1
					;17.5 - 18
					SIF INRANGE(TIME, 1050, 1080)
						TCVAR:LOCAL:인력점 = P35샤워실
				CASE 2	 
					;18 - 18.5
					SIF INRANGE(TIME, 1080, 1110)
						TCVAR:LOCAL:인력점 = P35샤워실
				CASE 3
					;18.5 - 19
					SIF INRANGE(TIME, 1110, 1140)
						TCVAR:LOCAL:인력점 = P35샤워실
				CASE 4
					;19 - 19.5
					SIF INRANGE(TIME, 1140, 1170)
						TCVAR:LOCAL:인력점 = P35샤워실
				CASE 5
					;19.5- 20
					SIF INRANGE(TIME, 1170, 1200)
						TCVAR:LOCAL:인력점 = P35샤워실
			ENDSELECT
		ENDIF
ENDSELECT


