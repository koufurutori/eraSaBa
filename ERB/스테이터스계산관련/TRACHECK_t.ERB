;FileName_TRACHECK.ERB ----------------------------- Rev1. 00
;고통쾌락경험, 봉사쾌락경험의 체크
;CALL		USER
;ARG		0:캐릭터 NO
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@EXP_GOT_CHECK(ARG)
#DIM 기학증가량
IF FLAG:70 == 1 || CFLAG:장난
ELSE
;봉사계인, 봉사 쾌락의 체크
LOCAL = CUP:ARG:쾌C + CUP:ARG:쾌V + CUP:ARG:쾌A + CUP:ARG:쾌B + CUP:ARG:쾌M + CUP:ARG:공순 / 5
LOCAL:1 = 0


SELECTCASE CUP:ARG:욕정
	CASE IS < 100
		LOCAL = 0
	CASE 101 TO 1000
	CASE 1001 TO 5000
		LOCAL *= 2
	CASE 5001 TO 10000
		LOCAL *= 3
	CASE 10001 TO 30000
		LOCAL *= 4
	CASEELSE
		LOCAL *= 5
ENDSELECT



SIF TCVAR:ARG:기학플래그
	LOCAL /= 4
;봉사플래그가 서 있다
IF TCVAR:ARG:108
	;봉사 쾌락의 체크
	IF LOCAL >= 100000
		LOCAL:1 = 16
		TIMES CUP:ARG:공포 , 0.65
		TIMES CUP:ARG:반감 , 0.30
	ELSEIF LOCAL >= 50000
		LOCAL:1 = 12
		TIMES CUP:ARG:공포 , 0.70
		TIMES CUP:ARG:반감 , 0.40
	ELSEIF LOCAL >= 30000
		LOCAL:1 = 8
		TIMES CUP:ARG:공포 , 0.75
		TIMES CUP:ARG:반감 , 0.50
	ELSEIF LOCAL >= 10000
		LOCAL:1 = 4
		TIMES CUP:ARG:공포 , 0.80
		TIMES CUP:ARG:반감 , 0.60
	ELSEIF LOCAL >= 5000
		LOCAL:1 = 2
		TIMES CUP:ARG:공포 , 0.85
		TIMES CUP:ARG:반감 , 0.70
	ELSEIF LOCAL >= 3000
		LOCAL:1 = 1
		TIMES CUP:ARG:공포 , 0.90
		TIMES CUP:ARG:반감 , 0.80
	ENDIF
	EXP:ARG:봉사쾌락경험 += LOCAL:1
ENDIF

;상대의 기학 쾌락의 체크
IF TCVAR:ARG:기학플래그
	LOCAL = CUP:ARG:쾌C + CUP:ARG:쾌V + CUP:ARG:쾌A + CUP:ARG:쾌B + CUP:ARG:쾌M
	SELECTCASE CUP:ARG:우월
		CASE IS < 100
			LOCAL = 0
		CASE 101 TO 1000
		CASE 1001 TO 5000
			LOCAL *= 2
		CASE 5001 TO 10000
			LOCAL *= 3
		CASE 10001 TO 30000
			LOCAL *= 4
		CASEELSE
			LOCAL *= 5
	ENDSELECT
	;가학 소스가 없으면 반감
	SIF !SOURCE:ARG:가학
		LOCAL /= 2
	;봉사쾌락경험이 들어가는 경우 감소
	SIF TCVAR:ARG:108
		LOCAL /= 4
	SELECTCASE LOCAL
		CASE IS > 100000
			기학증가량 = 16
		CASE IS > 50000
			기학증가량 = 12
		CASE IS > 30000
			기학증가량 = 8
		CASE IS > 10000
			기학증가량 = 4
		CASE IS > 5000
			기학증가량 = 2
		CASE IS > 3000
			기학증가량 = 1
	ENDSELECT
	SIF ABL:ARG:새드끼 < ABL:MASTER:새드끼
		기학증가량 /= 2
	EXP:ARG:가학쾌락경험 += 기학증가량
	TCVAR:ARG:기학플래그 = 0
ENDIF
;고통 쾌락의 체크
LOCAL = 0
IF CUP:ARG:욕정 >= 3000 && CUP:ARG:고통 >= 2000
	LOCAL = 16
	TIMES CUP:ARG:공포 , 0.65
ELSEIF CUP:ARG:욕정 >= 2500 && CUP:ARG:고통 >= 1500
	LOCAL = 12
	TIMES CUP:ARG:공포 , 0.70
ELSEIF CUP:ARG:욕정 >= 1500 && CUP:ARG:고통 >= 1000
	LOCAL = 8
	TIMES CUP:ARG:공포 , 0.75
ELSEIF CUP:ARG:욕정 >= 1000 && CUP:ARG:고통 >= 500
	LOCAL = 4
	TIMES CUP:ARG:공포 , 0.80
ELSEIF CUP:ARG:욕정 >= 600 && CUP:ARG:고통 >= 300
	LOCAL = 2
	TIMES CUP:ARG:공포 , 0.85
ELSEIF CUP:ARG:욕정 >= 300 && CUP:ARG:고통 >= 100
	LOCAL = 1
	TIMES CUP:ARG:공포 , 0.90
ENDIF
IF LOCAL > 0 && ARG != PLAYER
	EXP:ARG:고통쾌락경험 += LOCAL

	;가학쾌락경험
	LOCAL = ABL:PLAYER:새드끼 + ABL:ARG:마조끼 - 1
	SIF LOCAL > 0
		EXP:PLAYER:가학쾌락경험 += LOCAL
ENDIF
ENDIF
